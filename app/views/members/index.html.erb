<h1>List of people</h1>

<table class="member">
  <col class="id" />
  <col class="name" />
  <tr>
    <th>Member ID</th>
    <th>Member Name</th>
  </tr>
  <% @members.each do |member| %>
  <tr>
    <td><%= member.member_id %></td>
    <td><%= link_to member.name, member %></td>
  </tr>
  <% end %>
</table>

